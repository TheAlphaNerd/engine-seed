{
  "name": "engine-seed",
  "version": "1.0.0",
  "description": "seed project for a browserified version of the Famous Engine",
  "browser": "src/index.js",
  "scripts": {
    "build": "browserify src/index.js -g uglifyify | uglifyjs --screw-ie8 -m -c dead_code,sequences,conditionals,booleans,unused,if_return,join_vars,drop_debugger > public/bundle.js",
    "watch": "watchify src/index.js -d -v -o public/bundle.js",
    "start": "npm-run-all build serve",
    "serve": "ecstatic public/ -p 1618",
    "dev": "npm-run-all --parallel watch serve",
    "test": "npm-run-all lint build",
    "lint:jscs": "jscs src/",
    "lint:eslint": "eslint --ignore-path .gitignore src/",
    "lint": "npm-run-all lint:jscs lint:eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Famous/engine-seed.git"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "browserify": "^11.0.0",
    "ecstatic": "^0.8.0",
    "eslint": "^1.0.0",
    "jscs": "^2.0.0",
    "npm-run-all": "^1.2.6",
    "uglify-js": "^2.4.15",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  },
  "author": "Famous",
  "license": "MIT",
  "dependencies": {
    "babelify": "^6.0.1",
    "famous": "^0.7.1"
  }
}
